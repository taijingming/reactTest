#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_egg-scripts@2.13.0@egg-scripts/bin/egg-scripts.js" "$@"
  ret=$?
else 
  node  "$basedir/../_egg-scripts@2.13.0@egg-scripts/bin/egg-scripts.js" "$@"
  ret=$?
fi
exit $ret
